{
  "extends": "../../tsconfig.options.json",
  "include": [
    "**/*",
    "../../packages/ecs/**/*",
    "../../packages/engine/**/*",
    "../../packages/math/**/*",
    "../../packages/webgl/**/*"
  ],
  "references": [
    {
      "path": "../../packages/ecs"
    },
    {
      "path": "../../packages/engine"
    },
    {
      "path": "../../packages/math"
    },
    {
      "path": "../../packages/webgl"
    }
  ],
  "compilerOptions": {
    "noEmit": true,
    "rootDir": "src",
    "paths": {
      "@playground/math/*": [
        "../../packages/math/src/*"
      ],
      "@playground/ecs/*": [
        "../../packages/ecs/src/*"
      ],
      "@playground/webgl/*": [
        "../../packages/webgl/src/*"
      ],
      "@playground/engine/*": [
        "../../packages/engine/src/*"
      ]
    },
    "outDir": "../../.moon/cache/types/apps/stress-test"
  }
}
